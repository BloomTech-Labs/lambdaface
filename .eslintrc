{
  "extends": [ "airbnb", "google", "eslint:recommended" ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "func-names": 0,
    "react/react-in-jsx-scope": 0,
    "require-jsdoc": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "linebreak-style": 0,
    "arrow-body-style": 0,
    "no-plusplus": 0,
    "react/prop-types": 0
  }
}